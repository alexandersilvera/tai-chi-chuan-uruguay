---
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="es">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <title>{title}</title>
    </head>
    <body class="bg-black text-white">
        <slot />
    </body>
</html> 